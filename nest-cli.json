{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api_gateway/src",
  "monorepo": true,
  "root": "apps/api_gateway",
  "compilerOptions": {
    "assets": [
      "**/*.hbs"
    ],
    "webpack": true,
    "tsConfigPath": "apps/api_gateway/tsconfig.app.json"
  },
  "projects": {
    "loan": {
      "type": "application",
      "root": "apps/api_gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api_gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api_gateway/tsconfig.app.json"
      }
    },


    "mailsender_service": {
